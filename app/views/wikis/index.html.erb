<table class="table table-hover">
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.wiki.name') %></th>
      <th><%= t('activerecord.attributes.wiki.user') %></th>
    </tr>
  </thead>
  <tbody>
  <% @wikis.each do |wiki| %>
    <tr>
      <td><%= link_to wiki.name, wiki_path(@parent, wiki) %></td> 
      <td>
        <%= link_to user_path(wiki.user) do %>
          <%= image_tag wiki.user.image, size: '50x50', class: 'face' %>
          <%= wiki.user.name %>
        <% end %> 
      </td> 
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to t('.new'), new_wiki_path(@parent), :class => "btn btn-warning btn-large new_group" %>
